{
  "name": "nuxt-db",
  "version": "0.2.7",
  "description": "JSON database module for Nuxt",
  "repository": "https://github.com/hardpixel/nuxt-db",
  "author": "Jonian Guveli",
  "license": "MIT",
  "private": false,
  "main": "./src/module.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev example",
    "dev:build": "nuxi build example",
    "dev:generate": "nuxi generate example",
    "dev:preview": "nuxi preview example",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare example"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0",
    "@xmldom/xmldom": "^0.9.8",
    "csvtojson": "^2.0.10",
    "destr": "^1.2.2",
    "fuzzysort": "^2.0.4",
    "gray-matter": "^4.0.3",
    "hookable": "^5.5.3",
    "html-tags": "^3.3.1",
    "js-yaml": "^4.1.0",
    "json5": "^2.2.3",
    "markdown-it": "^13.0.1",
    "ohash": "^1.1.2",
    "pathe": "^1.1.0",
    "picodb": "^1.0.5",
    "sort-on": "^5.2.0",
    "ufo": "^1.1.2",
    "unstorage": "^1.6.1",
    "xml2js": "^0.6.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "0.2.1",
    "@nuxt/schema": "^3.0.0",
    "@nuxtjs/eslint-config": "^12.0.0",
    "eslint": "^8.41.0",
    "nuxt": "^3.0.0"
  }
}
